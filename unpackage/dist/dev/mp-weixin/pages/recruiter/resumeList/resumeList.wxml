<view class="attachmentResume_page"><view class="resume_wrap"><view class="wrap_top"><view class="title">我收到的简历</view><view class="resume_num">{{"共"+deliverList.length+"份"}}</view></view><block wx:for="{{deliverList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="reume_item"><image src="../../../static/file.png" mode="widthFix"></image><block wx:if="{{item.type==1}}"><view data-event-opts="{{[['tap',[['openPdf',['$0'],[[['deliverList','',index,'resumeUrl']]]]]]]}}" class="resume_info" bindtap="__e"><view class="resume_name">{{item.resumeName}}</view><view class="resume_date">{{item.deliverDate}}</view></view></block><block wx:if="{{item.type==0}}"><view data-event-opts="{{[['tap',[['toOnlineResume',['$0'],[[['deliverList','',index,'deliverId']]]]]]]}}" class="resume_info" bindtap="__e"><view class="resume_name">{{item.deliverName+"在线简历"}}</view><view class="resume_date">{{item.deliverDate}}</view></view></block></view></block></view></view>